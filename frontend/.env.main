# Production/Main Environment Variables for agent2_ingestor

# Environment Detection
REACT_APP_ENVIRONMENT=main
REACT_APP_AMPLIFY_ENV=main

# API Configuration
REACT_APP_API_URL=https://api.agent2-ingestor.com
REACT_APP_AWS_REGION=us-east-1

# Cognito Configuration - MAIN Environment
REACT_APP_COGNITO_USER_POOL_ID_MAIN=REPLACE_WITH_ACTUAL_MAIN_USER_POOL_ID
REACT_APP_COGNITO_CLIENT_ID_MAIN=REPLACE_WITH_ACTUAL_MAIN_CLIENT_ID

# Application Configuration
REACT_APP_NAME=agent2_ingestor
REACT_APP_VERSION=1.0.0

# Feature Flags
REACT_APP_ENABLE_MONITORING=true
REACT_APP_DEBUG=false

# Build Configuration
GENERATE_SOURCEMAP=false